{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.12.2",
    "@material-ui/icons": "^4.11.2",
    "@types/react-dom": "^17.0.9",
    "axios": "^0.21.1",
    "mobx": "^5.15.7",
    "mobx-react": "^5.4.4",
    "mobx-react-router": "^4.1.0",
    "node-sass": "^4.14.1",
    "query-string": "^6.14.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.0.1",
    "rxjs": "^6.6.7",
    "styled-components": "^4.4.1",
    "typescript": "^4.4.2"
  },
  "scripts": {
    "start": "cross-env PORT=3001 SKIP_PREFLIGHT_CHECK=true react-app-rewired start",
    "build": "cross-env react-app-rewired build",
    "test": "cross-env react-app-rewired test --env=jsdom",
    "eject": "cross-env react-scripts eject"
